{
    "name": "narad1972/TwitterClient",
    "type": "library",
    "description": "A Twitter API Client in PHP",
    "keywords": [
        "narad1972",
        "TwitterClient"
    ],
    "homepage": "https://github.com/narad1972/TwitterClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Nir Arad",
            "email": "narad1972@gmail.com",
            "homepage": "https://www.linkedin.com/in/narad/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "narad1972\\TwitterClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "narad1972\\TwitterClient\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
